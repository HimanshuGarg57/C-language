#include<stdio.h>

int main()
{
    int day ;
    printf("enter day 1-7");
    scanf("%d",&day);

    switch (day){
    
        case 1: printf("monday");
        break;       
        case 2: printf("we");
        break;       
        case 3: printf("m") ; 
        break;      
        case 4: printf("modd") ; 
        break;      
        case 5: printf("mon") ;  
        break;     
        case 6: printf("monn");   
        break;     
        case 7: printf("morr") ;  
        break;     
        default:printf("not valid");
    }
}

